extends templates/main
block variables
	- let pageTitle = TITLE_PART + '| Home parallax';
	- let isHome = true; //- Если true то логотип не будет ссылкой
	- let isDark = false;
	- let currentPage = 1.05; // Для каждой страницы уникален. Номер смотреть в файле pug/templates/_vars.pug в массивах с пунктами меню
	- let currentSection = 1; // // Для каждой раздела уникален. Home -1, projects - 2, news - 3, pages - 4

block header
	include modules/header

block content
	#pagepiling.homepage-parallax
		// Section 1
		section.section.parallax.pp-scrollable
			.scroll-wrap
				.container.parallax__container
					.parallax__wrapper
						.parallax__project
							.parallax__tag Architecture
							.parallax__title House Near Wrocklow
							a.parallax__link(href= isDark ? URLS.projects.detailDark : URLS.projects.detail, target='_blank')
								| See project
								+icon('chevron-right')(width="20", height="20")
						.parallax__counter
							span.current 01
							span.total / 05
		// Section 2
		section.section.parallax.pp-scrollable
			.scroll-wrap
				.container.parallax__container
					.parallax__wrapper
						.parallax__project
							.parallax__tag Architecture
							.parallax__title House Near Wrocklow
							a.parallax__link(href= isDark ? URLS.projects.detailDark : URLS.projects.detail, target='_blank')
								| See project
								+icon('chevron-right')(width="20", height="20")
						.parallax__counter
							span.current 01
							span.total / 05


		// Our story


block footer
	include modules/footer
